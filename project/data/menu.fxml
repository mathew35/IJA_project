<?xml version="1.0" encoding="UTF-8"?>

<?import de.jensd.fx.glyphs.fontawesome.FontAwesomeIconView?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="405.0" prefWidth="698.0" xmlns="http://javafx.com/javafx/11" xmlns:fx="http://javafx.com/fxml/1" fx:controller="project.MenuController">
  <children>
      <VBox alignment="CENTER" prefHeight="404.0" prefWidth="698.0">
         <children>
            <Button alignment="CENTER" mnemonicParsing="false" onAction="#switchToEditor" prefHeight="38.0" prefWidth="302.0" text="New Diagram...">
               <graphic>
                  <FontAwesomeIconView fill="#0dc92d" glyphName="PLUS" />
               </graphic>
               <VBox.margin>
                  <Insets bottom="20.0" />
               </VBox.margin>
            </Button>
            <Button alignment="CENTER" mnemonicParsing="false" onAction="#openClass" prefHeight="38.0" prefWidth="302.0" text="Open Class Diagram...">
               <graphic>
                  <FontAwesomeIconView fill="#0d00ff" glyphName="EJECT" />
               </graphic>
               <VBox.margin>
                  <Insets bottom="20.0" />
               </VBox.margin>
            </Button>
            <Button alignment="CENTER" disable="true" mnemonicParsing="false" onAction="#openSequence" opacity="0.0" prefHeight="38.0" prefWidth="302.0" text="Open Sequence Diagram...">
               <graphic>
                  <FontAwesomeIconView fill="#0d00ff" glyphName="EJECT" />
               </graphic>
            </Button>
         </children>
         <VBox.margin>
            <Insets />
         </VBox.margin>
      </VBox>
  </children>
</VBox>
